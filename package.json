{
  "name": "JobifyBackend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "setup-project": "yarn && cd Client && yarn",
    "server": "nodemon server.js",
    "client": "cd client && yarn run dev",
    "dev": "concurrently --kill-others-on-fail \" yarn run server\" \" yarn run client\"",
    "setup-production-app": "yarn && cd client && yarn && yarn run build "
